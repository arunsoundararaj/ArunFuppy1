<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <TextView
        android:layout_width="600dp"
        android:layout_height="150dp"
        android:fontFamily="700px"
        android:text="is  safe in his new home  ?"
        android:id="@+id/textView2"
        android:textSize="@dimen/abc_text_size_display_1_material"
        android:textAlignment="center"
        android:layout_above="@+id/imageButton2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="65dp"
        android:layout_height="40dp"
        android:id="@+id/imageButton2"
        android:src="@drawable/NewYes"
        android:clickable="true"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="46dp"
        android:layout_marginStart="46dp" />


    <ImageButton
        android:layout_width="65dp"
        android:layout_height="40dp"
        android:text="Call"
        android:id="@+id/imageButton"
        android:src="@drawable/NewNo"
        android:clickable="true"
        android:layout_marginRight="60dp"
        android:layout_marginEnd="60dp"
        android:layout_alignTop="@+id/imageButton2"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true">
    </ImageButton>

    <ImageButton
        android:layout_width="195dp"
        android:layout_height="30dp"
        android:id="@+id/imageButton3"
        android:src="@drawable/new_contactFuppy"
        android:clickable="true"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="92dp" />

    <ImageButton
        android:layout_width="195dp"
        android:layout_height="30dp"
        android:id="@+id/imageButton4"
        android:src="@drawable/ContactSeller"
        android:layout_alignBottom="@+id/imageButton3"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:layout_width="100dp"
        android:layout_height="60dp"
        android:src="@drawable/logo"
        android:id="@+id/imageView4"
        android:layout_alignParentBottom="true"
        android:layout_alignRight="@+id/imageButton4"
        android:layout_alignEnd="@+id/imageButton4" />


</RelativeLayout>
